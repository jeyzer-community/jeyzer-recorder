<?xml version="1.0" encoding="UTF-8" ?>
<agent>
	<!-- Variables do support system property and system environment variable resolution -->
    <variable name="jeyzer-agent-home">${JEYZER_AGENT_HOME}</variable>
    <delegate>org.jeyzer.recorder.JeyzerRecorderAgent</delegate>
    <classpath>
        <entry>${jeyzer-agent-home}/lib/commons-compress-1.19.jar</entry>
        <entry>${jeyzer-agent-home}/lib/jeyzer-recorder.jar</entry>
        <entry>${jeyzer-agent-home}/lib/jeyzer-publish.jar</entry>
        <entry>${jeyzer-agent-home}/lib/logback-classic-1.2.3.jar</entry>
        <entry>${jeyzer-agent-home}/lib/logback-core-1.2.3.jar</entry>
        <entry>${jeyzer-agent-home}/lib/slf4j-api-1.7.25.jar</entry>
		<entry>${jeyzer-agent-home}</entry>
		<entry>${jeyzer-agent-home}/config</entry>
    </classpath>
    <configuration>
        <item key="jeyzer-recorder-agent.props">${jeyzer-agent-home}/config/agent/jeyzer-record.properties</item>
    </configuration>
</agent>